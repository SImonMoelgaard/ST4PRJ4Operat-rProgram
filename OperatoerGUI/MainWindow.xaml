<Window x:Class="OperatoerGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wpf="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <wpf:CartesianChart Series="{Binding Linechart}" LegendLocation="Right" Margin="451,217,21,20">
            <wpf:CartesianChart.AxisY>
                <wpf:Axis Title="Amplitude" LabelFormatter="{Binding YFormatter}" />
            </wpf:CartesianChart.AxisY>
            <wpf:CartesianChart.AxisX>
                <wpf:Axis Title="Tid" Labels="{Binding Labels}" />
            </wpf:CartesianChart.AxisX>
        </wpf:CartesianChart>
    </Grid>
</Window>